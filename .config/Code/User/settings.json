{
    "workbench.colorTheme": "Atom One Dark",
    "window.zoomLevel": 2,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.tabSize": 2,
    "editor.wordWrap": "on",
    "haskell.serverExecutablePath": "/Users/danielwinogradcort/.ghcup/bin/haskell-language-server-wrapper-1.6.1.1",
    "explorer.confirmDelete": false,
    "todo-tree.regex.regex": "(--|//|#|<!--|;|/\\*|^|^[ \\t]*(-|\\d+.))\\s*($TAGS)",
    "rewrap.wrappingColumn": 80,
    "rewrap.doubleSentenceSpacing": true,
    "explorer.autoReveal": false,
    "scm.autoReveal": false,
    "gitlens.currentLine.enabled": false,
    "haskell.plugin.splice.globalOn": false,
    "editor.fontSize": 14,
    "files.trimTrailingWhitespace": true,
    "git.confirmSync": false,
    "editor.renderWhitespace": "none",
    "[haskell]": {
        "editor.defaultFormatter": "haskell.haskell"
    },
    "extensions.ignoreRecommendations": true,
    "customizeUI.titleBar": "inline",
    "customizeUI.activityBar": "bottom",
    "haskell.plugin.ghcide-completions.config.snippetsOn": false,
    "haskell.plugin.ghcide-completions.config.autoExtendOn": false,
    "todo-tree.general.tags": [
        "BUG",
        "HACK",
        "FIXME",
        "TODO",
        "XXX",
        "[ ]",
        "[x]",
        "CRITICAL",
        "REFACTOR"
    ],
    "haskell.plugin.eval.config.exception": true,
    "workbench.editor.pinnedTabSizing": "shrink",
    "workbench.editor.tabCloseButton": "left",
    "editor.copyWithSyntaxHighlighting": false,
    "diffEditor.ignoreTrimWhitespace": false,
    "window.titleBarStyle": "native",
    "editor.acceptSuggestionOnEnter": "off",
    "workbench.editor.untitled.labelFormat": "name",
    "editor.cursorBlinking": "solid",
    "vsicons.dontShowNewVersionMessage": true,
    "workbench.iconTheme": "vscode-icons"
}
