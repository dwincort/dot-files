# This is Git's per-user configuration file.
[user]
# Please adapt and uncomment the following lines:
  name = Daniel.Winograd-Cort

[merge]
  conflictstyle = diff3

[alias]
  st = status
  co = checkout
	ci = commit
  history = "log -p"
	unstage = reset --soft HEAD^
	pullp = pull --prune
  delete-local-branches = "!git branch -vv | grep ' gone]' | awk '{print $1}' | xargs git branch -d"
